{
  "env": {
    "browser": true
  },
  "globals": {
    "_": false,
    "Backbone": false,
    "jQuery": false,
    "wp": false
  },
  "rules": {
    "no-cond-assign": [
      2,
      "except-parens"
    ],
    "curly": 2,
    "eqeqeq": 2,
    "no-eq-null": 2,
    "no-unused-expressions": 2,
    "wrap-iife": [
      2,
      "any"
    ],
    "no-caller": 2,
    "quotes": [
      2,
      "single"
    ],
    "no-undef": 2,
    "no-unused-vars": 2,
    "no-multi-str": 2
  }
}
